<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefWidth="350.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.StatisticController">
    <children>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <TextField fx:id="startTextField" maxWidth="1.7976931348623157E308">
                    <VBox.margin>
                        <Insets left="8.0" right="8.0" top="8.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="endTextField" maxWidth="1.7976931348623157E308">
                    <VBox.margin>
                        <Insets left="8.0" right="8.0" top="8.0" />
                    </VBox.margin>
                </TextField>
                <ComboBox fx:id="serviceComboBox" maxWidth="1.7976931348623157E308">
                    <VBox.margin>
                        <Insets left="8.0" right="8.0" top="8.0" />
                    </VBox.margin>
                </ComboBox>
                <Button fx:id="resultButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Посчитать">
                    <VBox.margin>
                        <Insets left="8.0" right="8.0" top="8.0" />
                    </VBox.margin>
                </Button>
                <Label fx:id="amountPaymentLabel">
                    <VBox.margin>
                        <Insets left="8.0" right="8.0" top="8.0" />
                    </VBox.margin>
                </Label>
                <Label fx:id="sumContractsLabel" maxWidth="1.7976931348623157E308">
                    <VBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                    </VBox.margin>
                </Label>
            </children>
        </VBox>
    </children>
</AnchorPane>
