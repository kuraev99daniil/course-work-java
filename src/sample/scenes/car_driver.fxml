<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefWidth="400.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.detail.CarDriverController">
    <children>
        <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <TextField fx:id="surnameTextView">
                    <VBox.margin>
                        <Insets left="8.0" right="8.0" top="8.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="nameTextView" layoutX="10.0" layoutY="10.0">
                    <VBox.margin>
                        <Insets left="8.0" right="8.0" top="8.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="middleNameTextView" layoutX="10.0" layoutY="36.0">
                    <VBox.margin>
                        <Insets left="8.0" right="8.0" top="8.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="seriesTextView" layoutX="10.0" layoutY="61.0">
                    <VBox.margin>
                        <Insets left="8.0" right="8.0" top="8.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="numberTextView" layoutX="10.0" layoutY="87.0">
                    <VBox.margin>
                        <Insets left="8.0" right="8.0" top="8.0" />
                    </VBox.margin>
                </TextField>
                <HBox prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="applyButton" mnemonicParsing="false" text="Применить">
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="cancelButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Отмена">
                            <HBox.margin>
                                <Insets left="8.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                    <VBox.margin>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                    </VBox.margin>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
